<% var nationName = nation.get('name') %>

<div id="sidebar">
  <strong><a href="" class="nation-link"><%= nationName %></a></strong><br>
  <a href="" class="logout">Log Out</a>
</div>

<div id="issues">
  <h2 class="issue-header"><%= nationName %> Decides!</h2>
  <p>The following issues confront <%= nationName %>:</p>
  <ul class="issue-list">
    <% issues.each(function (issue) { %>
      <li><a href="#issue/<%= issue.id %>"><%= issue.get('title') %></a></li>
    <% }) %>
  </ul>
</div>
